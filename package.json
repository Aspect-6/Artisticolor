{
  "name": "artisticolor",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm:dev\" \"npm:watch\"",
    "serve": "PORT=8000 serve dist",
    "build": "webpack",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ext": "html ts tsx d.ts scss",
    "exec": "npm run build && clear"
  },
  "bugs": {
    "url": "https://github.com/Aspect-6/Artisticolor/issues"
  },
  "homepage": "https://github.com/Aspect-6/Artisticolor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aspect-6/Artisticolor.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@firebase/app": "^0.9.10",
    "@firebase/auth": "^0.23.2",
    "@firebase/database": "^0.14.4",
    "@firebase/firestore": "^4.1.0",
    "@types/crypto-js": "^4.1.2",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "bootstrap": "^5.3.0",
    "crypto-js": "^4.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "nodemon": "^3.0.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.0",
    "serve": "^14.2.0",
    "ts-loader": "^9.4.3",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.0.2"
  },
  "main": "./src/index.tsx",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
